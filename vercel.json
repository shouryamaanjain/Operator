{
  "version": 2,
  "rewrites": [
    {
      "source": "/chat/:path*",
      "destination": "https://api.emplode.com/chat/:path*"
    }
  ],
  "headers": [
    {
      "source": "/chat/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        }
      ]
    }
  ]
}

